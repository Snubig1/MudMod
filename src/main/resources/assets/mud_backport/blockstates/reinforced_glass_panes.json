{
  "multipart": [
    {
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "mud_backport:block/reinforced_glass_panes_noside",
        "y": 270
      }
    }
  ]
}